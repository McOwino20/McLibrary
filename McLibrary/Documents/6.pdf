<script>
    /*global $ */
    $(document).ready(function() {
      //alert("yo");
      "use strict";
      
    
         
    });  
    
     function login(){

                    
                    var base_url = $('#base_url').val();
                    var url=base_url + 'assets/assets/images/loaders/loader7.gif';
                    //alert(url);
                   
                    $('.msg_act').hide();

                    $('.message').addClass('alert alert-danger').html('<img src="'+ url + '"/> <span style="font-weight: bold; padding-left: 1em;font-size: 12px;">Processing please wait..!</span>').show();
                    $(':input').css('border-color',''); 
                    var email_add = $('#form-email_add').val();
                    var password = $('#form-password').val();
                    //die();
                    //alert(email_add + password);
                                
                    /*Validate email and password*/
                    if((email_add === '') ||(password === '')){
                        if((email_add === '')){
                            $('#form-email_add').css({'border-color':'red','box-shadow':'0 1px 1px #e8c8c8 inset'})
                            $('.message').show().removeClass('info cyan').addClass('alert alert-danger alert-dismissible mb-2').html(' Enter your email address !').focus();
                            $('#form-email_add').focus();
                        }
                        else if((password === '')){
                            $('#form-password').css('border-color','red');
                            $('.message').show().removeClass('info cyan').addClass('alert alert-danger alert-dismissible mb-2').html(' Enter your password!').focus();
                            $('#form-password').focus();
                        }
                                      
                    }else{
        
                        //Prepare Ajax call
                        //alert("https://studentportal.helb.co.ke/index.php/auth/signin");
                        $.post("https://studentportal.helb.co.ke/auth/signin",  
                        $("form").serialize(),
                        function(result) {
                          var data = result;

                          //alert(data);
                          var s =" ["+data+"]";
                          var myObject = eval('(' + s + ')');
                          for (i in myObject)
                          {
                              for (f in myObject[i] ){
                                  //alert("yo"+(myObject[i][f]));
                                  if (f === 'info'){
                                      if ((myObject[i][f])=== 'warning'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('The password entered is incorrect!')
                                            $('#form-password').css('border-color','red');
                                      }
                                      else if ((myObject[i][f])=== 'error'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('A user with that email does not exist in the system!')
                                           $('#form-email_add').css('border-color','red');
                                           $('#form-email_add').focus();
                                      }
                                      else if ((myObject[i][f])=== 'deactivated'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Your account is currently deactivated! Contact the administrator for more details!')
                                            $('#form-email_add').css('border-color','red');
                                            $('#form-email_add').focus();
                                            $('#error_msg').hide(1000);
                                      }
                                      else if ((myObject[i][f])=== 'inactive'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Please activate your account using the link sent to your email address during registration!')
                                            $('#form-email_add').css('border-color','red');
                                            $('#form-email_add').focus();
                                            $('#error_msg').hide(1000);
                                      }
                                      else if ((myObject[i][f])=== 'verification'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Your Account is Pending Verification By HELB Team. You will get a notification of the outcome within an Hour. Incase of any Challenge get intouch <a href="mailto:jswanya@helb.co.ke">Here>></a>')
                                            $('#form-email_add').css('border-color','red');
                                            $('#form-email_add').focus();
                                            $('#error_msg').hide(1000);
                                      }
                                      else if ((myObject[i][f])=== 'inst_verification'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Your Account is Pending Verification By HELB INSTITUTION PORTAL Team. You will get a notification of the outcome within an Hour. Incase of any Challenge get intouch <a href="mailto:AKibugu@helb.co.ke">Here>></a>')
                                            $('#form-email_add').css('border-color','red');
                                            $('#form-email_add').focus();
                                            $('#error_msg').hide(1000);
                                      }
                                      else if ((myObject[i][f])=== 'invalid'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Please enter a valid email address!')
                                            $('#form-email_add').css('border-color','red');
                                            $('#form-email_add').focus();
                                            
                                      }
                                      else if ((myObject[i][f])=== 'inst_ban'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Your institution account is currently deactivated! Contact the administrator for more details!')
                                            $('#form-email_add').css('border-color','red');
                                            $('#error_msg').hide(1000);
                                      }
                                      else if ((myObject[i][f])=== 'user_ban'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('Your account has been banned from accessing the portal! Contact the administrator for more details!')
                                            $('#form-email_add').css('border-color','red');
                                            $('#error_msg').hide(1000);
                                      }
                                       else if ((myObject[i][f])==='inst_inactive'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('<i class="fa fa-warning"></i><b> Account Pending Activation</b> <br/>Your institution account is registered pending approval. An approval email will be sent to your email address within 24 hours after registration. Incase of any challenge contact us by sending an email to: <a href="mailto:info@oprian.co.ke">info@oprian.co.ke</a> for more details.')
                                            $('#form-email_add').css('border-color','red');
                                            $('#error_msg').hide(1000);
                                      }else if ((myObject[i][f])==='inst_deactived'){
                                            $('.message').show().removeClass('info').addClass('alert alert-danger ').html('<i class="fa fa-warning"></i><b> Deactivated</b> <br/>Your institution account is deactivated. Contact the administrator for more details.')
                                            $('#form-email_add').css('border-color','red');
                                            $('#error_msg').hide(1000);
                                      }
                                      else{
                                            
                                            window.location = "https://studentportal.helb.co.ke/"+myObject[i][f] ;
                                      }
                                  }
                                  else{
                                        // alert(f+' - '+myObject[i][f]);
                                        $('#form-'+f).css('border-color','red');
                                        $("label[for='form-"+f+"'] small").css('color','red').html(myObject[i][f])
                                  }
                              }
                          } 
                     },"html");
                  }

                   return false;
            }

                  //Login keyup event
                  $('#form-password').bind('keyup', function(e) {
                    
                    
                      if ( e.keyCode === 13 ) { // 13 is enter key

                          // Execute code here.
                          login(); //Call Login Function
                      }

                  });

                  /**
                  * Login in Form
                  */
                  //$('#form-login').unbind('submit').submit(function(e){
                  $( document ).on( "click", ".btn-signin", function(e) {
                      e.preventDefault();
                      
                      login(); //Call Login Function

                  });

                //Forgot password 
                //$('#reset-form').unbind('submit').submit(function(e){
                $( document ).on( "click", ".btn-forgot", function(e) {
                    e.preventDefault();

                    //alert('yo');
                    $('.message').removeClass('info').addClass('alert alert-danger').html('<img src="https://studentportal.helb.co.ke/assets/assets/images/loaders/loader7.gif" style="vertical-align:text-bottom;" alt="" /><span style="font-weight: bold; padding-left: 1em;font-size: 12px;">Processing please wait..!</span>').show();
                      $(':input').css('border-color',''); 
                      $(':input').css('box-shadow',''); 
                      $('label small').css('color','').html('');
                      
                      //var url=$('#url').val();
                      var email_add= $('#reset-email_add').val();
                    
                    /*Validate email*/
                    if(email_add === ''){
                        $('#reset-email_add').css({'border-color':'#a94442','box-shadow':'0 1px 1px #e8c8c8 inset'});
                        $('.message').show().removeClass('info').addClass('alert alert-danger ').html(' Enter your a email address !').focus();             
                    }else{        
                              
                              $(".btn").attr("disabled", true); //Disable Button

                              $.post("https://studentportal.helb.co.ke/auth/forgot",  
                              $("#reset-form").serialize(),
                              function(result) {
                                  //alert(result);
                                  $(".btn").attr("disabled", false); //Enable Button
                                  var data = result;
                                  var s =" ["+data+"]";
                                  var myObject = eval('(' + s + ')');
                                  for (i in myObject)
                                  {
                                      for (f in myObject[i] ){
                                          //alert("yo" + myObject[i][f]);
                                          
                                          if (f === 'info'){
                                              if ((myObject[i][f])=== 'warning'){
                                                    $('.message').show().removeClass('info').addClass('alert alert-danger ').html('A problem occurred when resetting your password, please try again later!')
                                                    //$('#reset-email_add').css({'border-color':'#a94442','box-shadow':'0 1px 1px #e8c8c8 inset'})

                                              }
                                              else if ((myObject[i][f])=== 'error'){
                                                    $('.message').show().removeClass('alert alert-info').addClass('alert alert-danger ').html('A user with that email does not exist in the system!')
                                                    //$('#reset-email_add').css({'border-color':'#a94442','box-shadow':'0 1px 1px #e8c8c8 inset'})

                                              }
                                              else if ((myObject[i][f])=== 'reset'){
                                                    $('.message').show().removeClass('alert alert-info alert-danger').addClass('alert alert-success ').html('<h4> <b>Success !</b> </h4> A link to reset your password has been sent to your email address!')
                                                   // $('#reset-email_add').css({'border-color':'#a94442','box-shadow':'0 1px 1px #e8c8c8 inset'})
                                                    $('#reset_msg').hide(1000);
                                              }
                                              else if ((myObject[i][f])=== 'invalid'){
                                                    //$('#reset-email_add').css({'border-color':'#a94442','box-shadow':'0 1px 1px #e8c8c8 inset'})
                                                    $('.message').show().removeClass('alert alert-info').addClass('alert alert-danger ').html('Please enter a valid email address!')    ;                     }
                                          }
                                          else{
                                              // alert(f+' - '+myObject[i][f]);
                                              $('#form-'+f).css('border-color','red');
                                              $("label[for='form-"+f+"'] small").css('color','red').html(myObject[i][f])
                                          }
                                      }
                                  } 
                             },"html");
                        }
                        
                        
                        e.preventDefault();
                        return false;
                    }); 

                //Reset user
                //$('#reset-form').unbind('submit').submit(function(e){
                $( document ).on( "click", ".btn-reset", function(e) {
                        
                      //$('.message h3').html('Processing...');
                      $('.message').removeClass('info').addClass('alert alert-danger').html('<img src="https://studentportal.helb.co.ke/assets/assets/images/loaders/loader7.gif" style="vertical-align:text-bottom;" alt="" /><span style="font-weight: bold; padding-left: 1em;font-size: 12px;">Processing please wait..!</span>');
                      $(':input').css('border-color',''); 
                      $(':input').css('box-shadow',''); 
                      $('label small').css('color','').html('');

                      //alert($('form').serialize());
                      //alert('https://studentportal.helb.co.ke/auth/reset/');
                      
                      $(".btn").attr("disabled", true); //Disable Button

                      $.post("https://studentportal.helb.co.ke/auth/reset/",  
                        $('form').serialize(),
                        function(result) {

                        $(".btn").attr("disabled", false); //Disable Button
                         var data = result;
                         //alert(data);
                         var s =" ["+data+"]";
                          var myObject = eval('(' + s + ')');
                          for (i in myObject)
                          {
                            for (f in myObject[i] ){
                              if (f === 'info'){
                                if ((myObject[i][f])=== 'success'){
                                    $('.message').show().removeClass('alert alert-danger alert-info').addClass('alert alert-success ').html('<h4><b>Congratulations!</b></h4> Your Password has been reset. <br />To login to your account: <a href="https://studentportal.helb.co.ke/auth/signin">Click Here >>></a>');
                                  $('#reset_msg').hide(1000);
                                }
                                else if ((myObject[i][f])=== 'error'){
                                  $('.message').removeClass('info').addClass('alert alert-danger');
                                  $('.message').show().html('<h5> <b>Reset Error! </b></h5> <p>Your Account has been reset. To reset again , please request a reset code by clicking on forgot password link in Login page. Incase of any challenges please contact the administrator.!</p>')
                                }
                                
                              } else{
                                //alert(f+' - '+myObject[i][f]);
                                $('#form-'+f).css({'border-color':'#a94442','box-shadow':'0 3px 3px #e8c8c8'})
                                $("label[for='form-"+f+"'] small").css('color','red').html('&nbsp;' + myObject[i][f])
                                $('.message h3').show().html('Oops!');
                                $('.message').removeClass('info').addClass('alert alert-danger').html('<b>Oops! Correct Following errors</b> <br />' + myObject[i][f]);
                                
                              }
                            }
                          } 
                         },"html");

                      e.preventDefault();
                      return false;
                }); 


                //Reset user Request
                //$('#reset-form').unbind('submit').submit(function(e){
                $( document ).on( "click", ".btn-reset-request", function(e) {
                      e.preventDefault();
                     //$('.message h3').html('Processing...');
                      $('.message').removeClass('info').addClass('alert alert-danger').html('<img src="https://studentportal.helb.co.ke/assets/assets/images/loaders/loader7.gif" style="vertical-align:text-bottom;" alt="" /><span style="font-weight: bold; padding-left: 1em;font-size: 12px;">Processing please wait..!</span>').show();
                      $(':input').css('border-color',''); 
                      $(':input').css('box-shadow',''); 
                      $('label small').css('color','').html('');
                      var url=$('#url').val();
                      //alert('https://studentportal.helb.co.ke/auth/reset/' + url );
                    
                      $(".btn").attr("disabled", true); //Disable Button

                      $.post("https://studentportal.helb.co.ke/auth/reset/" + url,  
                      $('form').serialize(),
                      function(result) {
                       $(".btn").attr("disabled", false);
                       
                        var data = result;
                        //alert(data);
                        var s =" ["+data+"]";
                        var myObject = eval('(' + s + ')');
                        for (i in myObject)
                        {
                          for (f in myObject[i] ){
                            if (f === 'info'){
                              if ((myObject[i][f])=== 'success'){
                                  $('.message').show().removeClass('alert alert-info alert-danger').addClass('alert alert-success ').html('<h4> <b>Congratulations! </b></h4> You have reset the password to your account on HELB Portal. To Login <a href="https://studentportal.helb.co.ke/auth/signin">CLICK HERE >> </a> ');
                                $('#reset_msg').hide(1000);
                              }
                              else if ((myObject[i][f])=== 'error'){
                                $('.message').removeClass('info').addClass('alert alert-danger');
                                $('.message').show().html('<h5> <b>Reset Error! </b></h5> <p>Your Account has been reset. To reset again , please request a reset code by clicking on forgot password link in Login page. Incase of any challenges please contact the administrator.!</p>')
                              }
                            }
                            else{
                              //alert(f+' - '+myObject[i][f]);
                              $('#form-'+f).css({'border-color':'#a94442','box-shadow':'0 3px 3px #e8c8c8'})
                              $("label[for='form-"+f+"'] small").css('color','red').html('&nbsp;' + myObject[i][f])
                              $('.message h3').show().html('Oops!');
                              $('.message').removeClass('info').addClass('alert alert-danger').html('<b>Oops!</b> Please Correct the errors highlighted below! <br />');
                              
                            }
                          }
                        } 
                       },"html");
                       
                        return false;
                });


                // Get the input field
                var input = document.getElementById("form-password");

                // Get the warning text
                var text = document.getElementById("msg");

                // When the user presses any key on the keyboard, run the function
                input.addEventListener("keyup", function(event) {

                  // If "caps lock" is pressed, display the warning text
                  if (event.getModifierState("CapsLock")) {
                    //alert('yo');
                    text.style.display = "block";
                    $('#msg').show();
                  } else {
                    text.style.display = "none"
                  }
                });


    </script>    
